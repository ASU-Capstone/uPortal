    <portletPreference> 
      <name>img-height</name>  
      <values><value>200</value></values>  
    </portletPreference>
    <portletPreference> 
      <name>img-link</name>  
      <values><value>Faculty_feature.png</value></values>  
    </portletPreference>
    <portletPreference> 
      <name>img-uri</name>  
      <values><value>media/org/jasig/portal/channels/CImage/faculty_feature.png</value></values>  
    </portletPreference>
    <portletPreference> 
      <name>img-width</name>  
      <values><value>468</value></values>  
