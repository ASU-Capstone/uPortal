<choose>
    <when test="${groovy('${valueOf(name)}'.equals('Everyone'))}">
        <return value="0" />
    </when>
    <when test="${groovy('${valueOf(name)}'.equals('All categories'))}">
        <return value="1" />
    </when>
    <when test="${groovy('${valueOf(name)}'.equals('Portal Administrators'))}">
        <return value="2" />
    </when>
    <otherwise>
        <return value="${org.jasig.portal.io.SequenceGeneratorPhrase(UP_GROUP)}" />
    </otherwise>
</choose>